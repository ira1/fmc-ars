            title: {
                text: "Musicians By County",
                style: {
                    color: (Highcharts.theme && Highcharts.theme.textColor) || 'black'
                }
            },
            floating: false,
        },
		credits: {
			enabled: false
		},
        mapNavigation: {
            enabled: true,
			buttonOptions: {
				verticalAlign: 'bottom'
			}
        },

        colorAxis: { 
			minColor: '#F6B179',
            maxColor: '#FF2C2E'
        },
		
        plotOptions: {
            mapline: {
                showInLegend: false,
                enableMouseTracking: false
            }
        },

        series : [{
            mapData : countiesMap,
            data: data,
            joinBy: 'fips',
            name: 'County of',
            tooltip: {
                valueSuffix: ' musicians'
